# game/sakuracan.rpy:4925 #-#-# translate crowdin obbbzzzaaalll_dcacfec1:
msgid "[_r_] Get some rest, Hinata, you have a lot of work tomorrow."
msgstr ""

# game/sakuracan.rpy:4929 #-#-# translate crowdin obbbzzzaaalll_00e46c51:
msgid "[_hi_] Sure... Good night!"
