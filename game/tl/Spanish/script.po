# game/script.rpy:8016 #-#-# translate crowdin lutoborz_3ce8d9b0:
msgid "[_r_] Well, you can ask any question that interest you, I have nothing to hide."
msgstr "[_r_] Bueno, puedes hacer cualquier pregunta que te interese, no tengo nada que esconder."

# game/script.rpy:8020 #-#-# translate crowdin lutoborz_cec125fd:
msgid "[_hi_] Okay... If you say so..."
